{
	"template": "mail-*",
	"settings": {
		"number_of_shards": 1,
		"number_of_replicas": 1
	},
	"mappings": {
		"properties": {
			"type": {
				"type": "keyword"
			},
			"email": {
				"type": "keyword"
			},
			"timestamp": {
				"type": "date"
			},
			"date": {
				"type": "date"
			},
			"event": {
				"type": "text"
			},
			"ip": {
				"type": "keyword"
			},
			"sg_event_id": {
				"type": "keyword"
			},
			"sg_message_id": {
				"type": "keyword"
			}
		}
	}
}